{
  "compilerOptions": {
    "types": ["react", "react-dom", "@types/react"],
    "lib": ["dom", "dom.iterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env=NODE_ENV --watch server.tsx & deno task tailwind:watch",
    "tailwind:watch": "./bin/tailwindcss-windows-x64.exe -i ./index.css -o ./static/style.css --watch"
  },
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.1.5",
    "@types/react": "npm:@types/react@^19.1.8",
    "@types/react-dom": "npm:@types/react-dom@^19.1.6",
    "autoprefixer": "npm:autoprefixer@^10.4.21",
    "lucide-react": "npm:lucide-react@^0.525.0",
    "postcss": "npm:postcss@^8.5.6",
    "react": "npm:react@^19.1.0",
    "react-dom": "npm:react-dom@^19.1.0",
    "tailwindcss": "npm:tailwindcss@^4.1.11"
  }
}
